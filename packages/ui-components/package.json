{
  "name": "@neo4j-cypher/ui-components",
  "private": true,
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "neo4j",
    "cypher",
    "react",
    "components",
    "visualization"
  ],
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "devs": "HTTPS_ENABLED=1 vite",
    "prod": "vite preview",
    "clean": "rm -rf dist && rm -rf node_modules",
    "build": "pnpm build:css && pnpm postbuild:css",
    "build:css": "npx tailwindcss -i ./src/components/nvl-graph-viz/nvl-styles.css -o ./dist/nvl-styles.css --minify",
    "postbuild:css": "cp ./dist/nvl-styles.css ../vscode-extension/resources/styles/nvl-styles.css",
    "benchmark": "BENCHMARKING=true npx playwright test -c playwright-ct.config.ts -g \"benchmarking & performance test session\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/neo4j/cypher-language-support.git"
  },
  "bugs": {
    "url": "https://github.com/neo4j/cypher-language-support/issues"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18.18.2"
  },
  "dependencies": {
    "@neo4j-ndl/base": "^3.4.1",
    "@neo4j-ndl/react": "^3.5.3",
    "@neo4j-nvl/base": "0.3.8",
    "@neo4j-nvl/react": "0.3.8",
    "@tailwindcss/vite": "^4.1.4",
    "classnames": "2.5.1",
    "neo4j-driver": "catalog:",
    "re-resizable": "6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.5.10"
  }
}
