{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "outputMode": "new-only"
    },
    "@neo4j-cypher/language-support#build": {
      "outputs": ["src/generated-parser/**", "dist/**"]
    },
    "@neo4j-cypher/react-codemirror-playground#dev": {
      "outputs": [],
      "dependsOn": ["^build"],
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "watch": {
      "cache": false
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "@neo4j-cypher/react-codemirror#test:e2e": {
      "cache": false,
      "persistent": false,
      "dependsOn": ["@neo4j-cypher/react-codemirror#build"]
    },
    "@neo4j-cypher/react-codemirror-playground#build": {
      "dependsOn": ["@neo4j-cypher/react-codemirror#build"]
    },
    "neo4j-vscode#test:e2e": {
      "cache": false,
      "persistent": false,
      "dependsOn": ["@neo4j-cypher/react-codemirror#test:e2e"]
    },
    "benchmark": {
      "cache": false
    }
  }
}
