{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["out/**"],
      "dependsOn": ["^build"]
    },
    "assemble": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "clean": {},
    "watch": {},
    "test": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "cache": false,
      "persistent": true
    },
    "codemirror-playground#dev": {
      "dependsOn": ["language-support#build"]
    }
  }
}
